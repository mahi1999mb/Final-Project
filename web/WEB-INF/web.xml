<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <servlet>
        <servlet-name>Owner</servlet-name>
        <servlet-class>KPABS.Owner_register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Owner</servlet-name>
        <url-pattern>/owner_reg</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>KPABS.User_register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/user_reg</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Cloud_login</servlet-name>
        <servlet-class>KPABS.Cloud</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Cloud_login</servlet-name>
        <url-pattern>/cloud</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TTP_login</servlet-name>
        <servlet-class>KPABS.TTP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TTP_login</servlet-name>
        <url-pattern>/TTP</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Data_upload</servlet-name>
        <servlet-class>KPABS.data_upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Data_upload</servlet-name>
        <url-pattern>/uploadfile</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Download_file</servlet-name>
        <servlet-class>KPABS.Download</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Download_file</servlet-name>
        <url-pattern>/Download</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
